{
  "name": "guilds-client",
  "version": "1.0.0",
  "description": "A minimal Electron application (with typescript)",
  "main": "target/app/main.js",
  "scripts": {
    "clean": "del-cli ./target",
    "compile": "npm run clean && npm run compile:main && npm run compile:renderer",
    "compile:main": "cross-env NODE_ENV=\"production\" webpack --config main/webpack.config.ts",
    "compile:renderer": "cross-env NODE_ENV=\"production\" webpack --config renderer/webpack.config.ts",
    "dist:linux": "electron-builder --config ./electron-builder.json --linux",
    "dist:mac": "electron-builder --config ./electron-builder.json --mac",
    "dist:windows": "electron-builder --config ./electron-builder.json --win",
    "lint": "eslint --ext .js,.ts,.svelte .",
    "lint:fix": "eslint --ext .js,.ts,.svelte . --fix",
    "postinstall": "electron-builder install-app-deps",
    "prestart": "npm run compile",
    "start:dev": "concurrently -n \"WebpackMain,WebpackRenderer\" -c \"cyan.bold,blue.bold\" \"webpack -w --config main/webpack.config.ts\" \"webpack -w --config renderer/webpack.config.ts\" -k",
    "start:app": "electron target/app/main.js"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@pyoner/svelte-types": "^3.4.4-2",
    "@types/copy-webpack-plugin": "^5.0.0",
    "@types/mini-css-extract-plugin": "^0.9.1",
    "@types/node": "^10.17.14",
    "@types/node-fetch": "^2.5.5",
    "@types/terser-webpack-plugin": "^2.2.0",
    "@types/webpack": "^4.41.7",
    "@types/ws": "^7.2.2",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "autoprefixer": "^9.7.4",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.4.2",
    "del-cli": "^3.0.0",
    "electron": "^8.1.1",
    "electron-builder": "^22.4.1",
    "eslint": "^6.8.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.9.0",
    "null-loader": "^3.0.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "style-loader": "^1.1.3",
    "svelte": "^3.20.1",
    "svelte-loader": "^2.13.6",
    "svelte-preprocess": "^3.5.0",
    "terser-webpack-plugin": "^2.3.5",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.7.0",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "author": "Antosik",
  "license": "MIT",
  "dependencies": {
    "electron-log": "^4.1.0",
    "electron-store": "^5.1.1",
    "electron-updater": "^4.2.5",
    "league-connect": "^2.0.1",
    "node-fetch": "^2.6.0",
    "svelte-spa-router": "^2.0.1"
  }
}