{
  "name": "guilds-client",
  "version": "0.1.1",
  "description": "This desktop application allows users to interact with the “Guilds” program in League of Legends.",
  "main": "target/app/main.js",
  "scripts": {
    "clean": "del-cli ./target",
    "compile": "npm run clean && npm run compile:main && npm run compile:renderer",
    "compile:main": "cross-env NODE_ENV=\"production\" webpack --config main/webpack.config.ts",
    "compile:renderer": "cross-env NODE_ENV=\"production\" webpack --config renderer/webpack.config.ts",
    "dist:linux": "electron-builder --config ./electron-builder.json --linux",
    "dist:mac": "electron-builder --config ./electron-builder.json --mac",
    "dist:windows": "electron-builder --config ./electron-builder.json --win",
    "lint": "concurrently -n \"ESLint,CSSLint\" -c \"cyan.bold,blue.bold\" \"npm run lint:js\" \"npm run lint:css\"",
    "lint:js": "eslint --ext .js,.ts,.svelte .",
    "lint:css": "stylelint renderer/**/*.{css,html,svelte}",
    "postinstall": "electron-builder install-app-deps",
    "prestart": "npm run compile",
    "start:dev": "concurrently -n \"WebpackMain,WebpackRenderer\" -c \"cyan.bold,blue.bold\" \"webpack -w --config main/webpack.config.ts\" \"webpack -w --config renderer/webpack.config.ts\" -k",
    "start:app": "electron ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Antosik/lol-guilds-client.git"
  },
  "homepage": "https://github.com/Antosik/lol-guilds-client",
  "bugs": {
    "url": "https://github.com/Antosik/lol-guilds-client/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.10.1",
    "@pyoner/svelte-types": "^3.4.4-2",
    "@types/copy-webpack-plugin": "^5.0.0",
    "@types/markdown-it": "^10.0.1",
    "@types/mini-css-extract-plugin": "^0.9.1",
    "@types/node": "^10.17.14",
    "@types/node-fetch": "^2.5.7",
    "@types/terser-webpack-plugin": "^2.2.0",
    "@types/webpack": "^4.41.13",
    "@types/ws": "^7.2.4",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "autoprefixer": "^9.8.0",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.2.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "del-cli": "^3.0.1",
    "electron": "^9.0.0",
    "electron-builder": "^22.7.0",
    "eslint": "^7.1.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "json-loader": "^0.5.7",
    "markdown-it-for-inline": "^0.1.1",
    "mini-css-extract-plugin": "^0.9.0",
    "null-loader": "^4.0.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "style-loader": "^1.2.1",
    "stylelint": "^13.5.0",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-standard": "^20.0.0",
    "svelte": "^3.23.0",
    "svelte-loader": "^2.13.6",
    "svelte-preprocess": "^3.7.4",
    "terser-webpack-plugin": "^3.0.2",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "author": "Antosik",
  "license": "MIT",
  "dependencies": {
    "date-fns": "^2.14.0",
    "electron-is-dev": "^1.2.0",
    "electron-log": "^4.2.0",
    "electron-store": "^5.1.1",
    "electron-updater": "^4.3.1",
    "league-connect": "^2.0.1",
    "markdown-it": "^11.0.0",
    "node-fetch": "^2.6.0",
    "svelte-spa-router": "^2.1.0"
  }
}
